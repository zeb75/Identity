@model IEnumerable<Identity.Models.City>

@{
    ViewBag.Title = "AddCity";
}

<h2>Add city to country</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @*Student Id*@  @*Course Id*@
            @Html.ActionLink("Add", "CityToCountry", new { sId = item.Id, cId = ViewBag.cId })
        </td>
    </tr>
}

</table>
